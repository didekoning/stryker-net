{
  "stryker-config": {
    "baseline": {
      "enabled": true,
      "provider": "AzureFileStorage",
      "azure-fileshare-url": "https://strykernetbaseline.file.core.windows.net/baselines"
    },
    "reporters": [
      "Dots",
      "ClearText",
      "Html",
      "Json"
    ],
    "project": "TargetProject.csproj",
    "ignore-mutations": [
      "string"
    ],
    "mutate": [
      "!Person.cs"
    ]
  }
}
